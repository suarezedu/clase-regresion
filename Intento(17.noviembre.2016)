require(foreign)
cuestionario<-data.frame(read.dbf("motral2012_empleos.dbf"))
sdem<-data.frame(read.dbf("sdemt212.dbf"))
sdem$EDA<-as.character(as.numeric(sdem$EDA))
View(sdem)
class(sdem$CD_A)
class(sdem$EDA)
g1<-subset(sdem,sdem$EDA==18| sdem$EDA==19)
g2<-subset(sdem,sdem$EDA>=20& sdem$EDA<=29)
g3<-subset(sdem,sdem$EDA>=30& sdem$EDA<=39)
g4<-subset(sdem,sdem$EDA>=40& sdem$EDA<=49)
g5<-subset(sdem,sdem$EDA>=50)
sdem$POS_OCU<-as.character(as.numeric(sdem$POS_OCU))
View(sdem$POS_OCU)
class(sdem$POS_OCU)
asalariados<-subset(sdem,sdem$POS_OCU==1)
noasalariados<-subset(sdem,sdem$POS_OCU==2|sdem$POS_OCU==3|sdem$POS_OCU==4)
View(asalariados)
nulos<-subset(sdem,sdem$POS_OCU==0)
View(nulos)
##MOTRAL
cuestionario$EDA<-as.character(as.numeric(cuestionario$EDA))
h1<-subset(cuestionario,cuestionario$EDA==18| cuestionario$EDA==19)
h2<-subset(cuestionario,cuestionario$EDA>=20& cuestionario$EDA<=29)
h3<-subset(cuestionario,cuestionario$EDA>=30& cuestionario$EDA<=39)
h4<-subset(cuestionario,cuestionario$EDA>=40& cuestionario$EDA<=49)
h5<-subset(cuestionario,cuestionario$EDA>=50)
class(cuestionario$P5)
View(cuestionario)
