setwd("C:\\Users\\Sala-C4\\Desktop")

require(foreign)
SDEMT216<-read.dbf("sdemt216.dbf")
COE1<-read.dbf("coe1t216.dbf")
COE2<-read.dbf("coe2t216.dbf")
folio<-paste(SDEMT216$CD_A,SDEMT216$ENT,SDEMT216$CON,SDEMT216$V_SEL,SDEMT216$N_HOG,SDEMT216$H_MUD,SDEMT216$N_REN)
View(folio)
x<-data.frame(SDEMT216,folio)
folio<-paste(COE1$CD_A,COE1$ENT,COE1$CON,COE1$V_SEL,COE1$N_HOG,COE1$H_MUD,COE1$N_REN)
x1<-data.frame(COE1,folio)
folio<-paste(COE2$CD_A,COE2$ENT,COE2$CON,COE2$V_SEL,COE2$N_HOG,COE2$H_MUD,COE2$N_REN)
x2<-data.frame(COE2,folio)
View(x2$folio)
total<-merge(x,x1,by="folio",all=TRUE)
View(total)
Enoetotal<-merge(total,x2,by="folio",all.x=TRUE)
ls(x1)
install.packages("questionr")
require(questionr)
table(Enoetotal$SEX,Enoetotal$P1)
wtd.table(Enoetotal$SEX,Enoetotal$P1,weights=Enoetotal$FAC)
